{
	"extends": "astro/tsconfigs/strict",
	"compilerOptions": {
		"outDir": "dist",
		"rootDir": ".",
		"declaration": true,
		"skipLibCheck": true,
		"resolveJsonModule": true,
		"jsx": "react-jsx",
		"types": ["astro/client"]
	},
	"include": [
		"src",
		".preview",
		"config.js",
		"rollup.config.js",
		"rollup.config.ui.js",
		"rollup.config.translation.js",
		"tailwind.config.js"
	]
}
