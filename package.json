{
	"name": "@devprotocol/clubs-plugin-awesome-onboarding",
	"version": "0.1.2",
	"type": "module",
	"description": "Clubs plugin for intengrating awesome user onboarding.",
	"main": "dist/index.js",
	"exports": {
		".": {
			"default": "./dist/index.js"
		}
	},
	"types": "./dist/index.d.ts",
	"files": [
		"dist/*.d.ts",
		"dist/*.js",
		"!**/*.test.*",
		"src"
	],
	"keywords": [
		"clubs-plugin"
	],
	"scripts": {
		"test": "ava || echo 'skip'",
		"preview": "astro --root .preview/preview.static dev",
		"build": "rollup -c",
		"pretest": "yarn build",
		"prebuild": "rimraf dist",
		"lint": "yarn lint:eslint && yarn lint:format",
		"lint:eslint": "eslint . --ext .ts,.js --fix",
		"lint:format": "prettier --write '**/*.{ts,js,tsx,jsx,astro,vue,svelte,json,md,yml}'",
		"prepack": "yarn test && yarn build",
		"prepare": "husky install",
		"typecheck": "astro check && tsc --noEmit && svelte-check && vue-tsc --noEmit --p tsconfig.vue.json"
	},
	"author": "",
	"license": "MPL-2.0",
	"dependencies": {
		"@devprotocol/util-ts": "4.0.0",
		"@dynamic-labs/ethereum": "^1.4.0",
		"@dynamic-labs/ethers-v6": "^1.0.0",
		"@dynamic-labs/sdk-react-core": "^1.4.0",
		"ramda": "0.29.1",
		"viem": "1"
	},
	"peerDependencies": {
		"@devprotocol/clubs-core": "^2.1.1 || ^3.0.0",
		"ethers": "^6.0.0",
		"react": "^18.0.0",
		"react-dom": "^18.0.0"
	},
	"devDependencies": {
		"@astrojs/check": "^0.5.0",
		"@astrojs/react": "^3.0.6",
		"@astrojs/svelte": "^5.0.0",
		"@astrojs/tailwind": "^5.0.0",
		"@astrojs/vue": "^4.0.0",
		"@ava/typescript": "4.1.0",
		"@devprotocol/clubs-core": "3.10.4",
		"@devprotocol/elements": "^1.1.0",
		"@rollup/plugin-typescript": "11.1.6",
		"@tailwindcss/typography": "^0.5.10",
		"@types/dotenv": "8.2.0",
		"@types/node": "20.12.7",
		"@types/ramda": "0.29.12",
		"@types/react": "^18.2.38",
		"@typescript-eslint/eslint-plugin": "7.6.0",
		"@typescript-eslint/parser": "7.6.0",
		"astro": "^4.0.0",
		"ava": "6.1.2",
		"dotenv": "16.4.5",
		"eslint": "8.57.0",
		"eslint-config-prettier": "9.1.0",
		"eslint-plugin-functional": "6.5.1",
		"ethers": "6.11.1",
		"husky": "9.0.11",
		"prettier": "3.2.5",
		"prettier-plugin-astro": "^0.13.0",
		"prettier-plugin-svelte": "^3.0.0",
		"prettier-plugin-tailwindcss": "^0.5.7",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"rimraf": "5.0.5",
		"rollup": "4.14.3",
		"rollup-plugin-dts": "^6.1.0",
		"rxjs": "^7.8.1",
		"sass": "1.75.0",
		"svelte": "^4.2.3",
		"svelte-check": "^3.6.0",
		"tailwindcss": "3.4.3",
		"typescript": "5.4.5",
		"vue": "3.4.22",
		"vue-tsc": "^2.0.0"
	},
	"ava": {
		"files": [
			"**/*.test.ts"
		],
		"typescript": {
			"rewritePaths": {
				"/": "dist/"
			},
			"compile": false
		}
	},
	"repository": {
		"type": "git",
		"url": "git+ssh://git@github.com/dev-protocol/clubs-plugin-awesome-onboarding.git"
	},
	"bugs": {
		"url": "https://github.com/dev-protocol/clubs-plugin-awesome-onboarding/issues"
	},
	"homepage": "https://github.com/dev-protocol/clubs-plugin-awesome-onboarding#readme"
}
